{
  "name": "@kiwigrid/ngx-daterangepicker",
  "version": "0.4.3",
  "description": "Angular Datepicker",
  "main": "bundles/ngx-daterangepicker.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "serve": "webpack-dev-server --config config/webpack.config.dev.js",
    "build:dev": "npm run clean && webpack --config config/webpack.config.dev.js --progress --bail",
    "build:prod": "npm run clean && webpack --config config/webpack.config.prod.js --progress --bail",
    "build:dist": "ngc -p tsconfig.dist.json && webpack --config config/webpack.config.dist.js --progress --bail",
    "start": "npm run serve",
    "clean": "rimraf dist/ src/ngfactory lib bundles index.d.ts index.js index.metadata.json ngx-daterangepicker.d.ts ngx-daterangepicker.metadata.json ngx-daterangepicker.js src/**/*.metadata.json src/**/*.d.ts src/**/*.js aot",
    "test": "karma start config/karma.conf.js",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor ./config/protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "ci": "npm run lint && npm test && npm run e2e",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "tslint": "tslint",
    "prepublish": "npm run build:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiwigrid/ngx-daterangepicker.git"
  },
  "keywords": [
    "Angular",
    "date",
    "datepicker",
    "range",
    "daterange",
    "daterangepicker"
  ],
  "author": "Norman Leyda <norman.leyda@kiwigrid.com>",
  "license": "MIT",
  "homepage": "https://github.com/kiwigrid/ngx-daterangepicker#readme",
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@ngtools/webpack": "^1.2.12",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.7",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "bulma": "^0.3.2",
    "codelyzer": "^3.0.0-beta.3",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.2",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.2",
    "to-string-loader": "^1.1.5",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "zone.js": "^0.7.7"
  },
  "dependencies": {
    "date-fns": "^1.28.0"
  }
}
